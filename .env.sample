# Next.js Environment Variables
# Copy this file to .env.local and fill in your values

# LIFF ID from LINE Developers Console
NEXT_PUBLIC_LIFF_ID=1234567890-abcdefgh

# Application Name
NEXT_PUBLIC_APP_NAME=L-Apply

# Firebase Functions Apply API URL
# For production: https://asia-northeast1-YOUR_PROJECT_ID.cloudfunctions.net/apply
# For development: http://localhost:5001/YOUR_PROJECT_ID/asia-northeast1/apply
NEXT_PUBLIC_APPLY_API_URL=https://asia-northeast1-your-project-id.cloudfunctions.net/apply
